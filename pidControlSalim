#define IN1 8
#define IN2 3
#define IN4 10
#define IN3 9
#define EN1 5
#define EN2 6

float kp;
float kd;
float ki;

float current_angle;
float desired_angle = 512;
float error, previousError;

unsigned long currentTime, previousTime;
float elapsedTime;

float setPoint;
float 
void setup()
{
   Serial.begin(9600);
  pinMode(A0, INPUT);
 
  pinMode(EN1 , OUTPUT);
  pinMode(EN2 , OUTPUT);
  pinMode(IN1 , OUTPUT);
  pinMode(IN2 , OUTPUT);
  pinMode(IN4 , OUTPUT);
  pinMode(IN3 , OUTPUT);

  currentTime = millis();
  elapsedTime = currentTime - previousTime;
 
 
}

void loop()
{
  sensor();
   error = setPoint - input;
}

void sensor()
{
  current_angle = analogRead(A0);
  Serial.println(current_angle);

  analogWrite(EN1, 255);
  analogWrite(EN2, 255);
}


